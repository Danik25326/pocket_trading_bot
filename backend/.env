
# Pocket Option API
POCKET_SSID= 
POCKET_DEMO=true

# Groq API
GROQ_API_KEY=
GROQ_MODEL=openai/gpt-oss-120b

# Налаштування сигналів
SIGNAL_INTERVAL=600  # 10 хвилин у секундах
MIN_CONFIDENCE=0.7   # Мінімальний шанс 70%
ASSETS=GBPJPY_otc,EURUSD_otc,USDJPY_otc
TIMEFRAMES=120       # 2 хвилини у секундах
LOG_LEVEL=INFO

# Навчання AI
FEEDBACK_ENABLED=true
MAX_SIGNALS_HISTORY=100
ACTIVE_SIGNAL_TIMEOUT=10  # 10 хвилин
MAX_SIGNALS_ON_SITE=6     # Максимум 6 сигналів на сайті

# Додати до існуючого .env
MAX_DURATION=5.0
SIGNAL_VALIDITY_MINUTES=10
SIGNAL_CLEANUP_COUNT=6
